<h1>メンバー一覧</h1>
<table class="table table-default">
  <tr>
    <th>名前</th>
    <th>代</th>
    <th>一言コメント</th>
    <th>email</th>
  </tr>
 
  <% @users.each do |user| %>
    <tr>
      <td><%= user.name %></td>
      <td><%= user.generation %></td>
      <td><%= user.description %></td>   
      <td><%= user.email %></td>
    </tr>
  <% end %>
</table>
