<h5>【レポート一覧】</h5>
  <table class="table">
    <tr>
	  <th>Title</th>
	  <th>Description</th>
	  <th>Author</th>
    </tr>
    <% @project.reports.each do |report| %>
    <tr>
	  <td><%= report.title %></td>
	  <td><%= report.text %></td>
	  <td><%= report.user_id %></td>
    </tr>
    <% end %>
  </table>
